<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

    <!--
        Resource: stock.location
    -->
        <record id="stock_location_Sabah" model="stock.location">
            <field name="name">MY Sabah</field>
            <field name="usage">view</field>
            <field name="location_id" ref="stock.stock_location_locations"/>
            <field name="company_id" ref="company_Malaysia"/>
        </record>

        <record id="stock_location_output_Sabah" model="stock.location">
            <field name="name">Output</field>
            <field name="location_id" ref="stock_location_Sabah"/>
            <field name="usage">internal</field>
            <field name="chained_location_type">customer</field>
            <field name="chained_auto_packing">transparent</field>
            <field name="chained_picking_type">out</field>
            <field name="chained_journal_id" ref="stock.journal_delivery"/>
            <field name="company_id" ref="company_Malaysia"/>
        </record>
        <record id="stock_location_stock_Sabah" model="stock.location">
            <field name="name">Stock</field>
            <field name="location_id" ref="stock_location_Sabah"/>
            <field name="company_id" ref="company_Malaysia"/>
        </record>
        <record id="stock_location_input_Sabah" model="stock.location">
            <field name="name">Input</field>
            <field name="location_id" ref="stock_location_Sabah"/>
            <field name="usage">production</field>
            <field name="chained_location_type">fixed</field>
            <field name="chained_location_id" ref="stock_location_stock_Sabah"/>
            <field name="chained_auto_packing">manual</field>
            <field name="chained_picking_type">internal</field>
            <!-- field name="chained_journal_id" ref="stock.journal_delivery"/ -->
            <field name="company_id" ref="company_Malaysia"/>
        </record>

    <!--
        Resource: stock.warehouse
    -->
        <record id="warehouse0_Sabah" model="stock.warehouse">
            <field name="name">MY Sabah</field>
            <field name="lot_input_id" ref="stock_location_input_Sabah"/>
            <field name="lot_stock_id" ref="stock_location_stock_Sabah"/>
            <field name="lot_output_id" ref="stock_location_output_Sabah"/>
            <field name="company_id" ref="company_Malaysia"/>
        </record>

    <!--
        Resource: sale.shop
    -->
        <record id="shop_Sabah" model="sale.shop">
            <field name="company_id" ref="company_Malaysia"/>
            <field name="warehouse_id" ref="warehouse0_Sabah"/>
            <field name="payment_default_id" ref="account.account_payment_term"/>
            <!--field name="pricelist_id" ref="product.list0"/ -->
            <field name="name">MY Sabah</field>
        </record>

    </data>
</openerp>
