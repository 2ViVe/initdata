<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="0">

    <!-- Basic Company  -->
        <record id="partner_HoldingsEuropeLtd" model="res.partner">
            <field name="name">Organo Gold Holdings Europe Ltd</field>
            <field name="ref">HE</field>
            <!-- Address and Company ID will be set later -->
            <field name="address" eval="[]"/>
            <field name="company_id" eval="None"/>
            <field name="customer" eval="False"/>
        </record>
        <record id="address_HoldingsEuropeLtd" model="res.partner.address">
            <field name="partner_id" ref="partner_HoldingsEuropeLtd"/>
            <field name="type">default</field>
            <field name="company_id" eval="None"/>
        </record>

        <!-- Currencies -->
        <record id="EUR_HoldingsEuropeLtd" model="res.currency">
            <field name="name">EUR</field>
            <field name="symbol">â‚¬</field>
            <field name="rounding">0.01</field>
            <field name="accuracy">4</field>
            <!-- Company ID will be set later -->
            <field name="company_id" eval="None"/>
        </record>
        <record id="rateEUR_HoldingsEuropeLtd" model="res.currency.rate">
            <field name="rate">1.0</field>
            <field name="currency_id" ref="EUR_HoldingsEuropeLtd"/>
            <field eval="time.strftime('%Y-01-01')" name="name"/>
        </record>

        <record id="company_HoldingsEuropeLtd" model="res.company">
            <field name="name">Organo Gold Holdings Europe Ltd</field>
            <field name="partner_id" ref="partner_HoldingsEuropeLtd"/>
            <field name="parent_id" ref="base.main_company"/>
            <field name="rml_footer1"></field>
            <field name="rml_footer2"></field>
            <field name="currency_id" ref="EUR_HoldingsEuropeLtd"/>
        </record>

        <record model="res.users" id="user_he">
            <field name="name">HE Test</field>
            <field name="login">he</field>
            <field name="password">he</field>
            <field name="signature">HE Test User</field>
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
            <field name="menu_id" ref="base.action_menu_admin"/>
            <field name="company_ids" eval="[(4, ref('company_HoldingsEuropeLtd'))]"/>
            <field name="groups_id" eval="[(6, 0, [ref('base.group_user'), ref('base.group_extended'), ref('base.group_partner_manager'), ref('base.group_sale_manager'), ref('stock.group_stock_manager'), ref('purchase.group_purchase_manager'), ref('base.group_multi_company'), ref('account.group_account_manager'), ref('base.group_system'), ref('base.group_hr_manager')])]"/>
        </record>

        <record id="partner_HoldingsEuropeLtd" model="res.partner">
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
        </record>
        <record id="address_HoldingsEuropeLtd" model="res.partner.address">
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
        </record>
        <record id="EUR_HoldingsEuropeLtd" model="res.currency">
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
        </record>

    <!--
        Resource: stock.location
    -->
        <record id="stock_location_HEDefault" model="stock.location">
            <field name="name">HE Default</field>
            <field name="usage">view</field>
            <field name="location_id" ref="stock.stock_location_locations"/>
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
        </record>

        <record id="stock_location_output_HEDefault" model="stock.location">
            <field name="name">Output</field>
            <field name="location_id" ref="stock_location_HEDefault"/>
            <field name="usage">internal</field>
            <field name="chained_location_type">customer</field>
            <field name="chained_auto_packing">transparent</field>
            <field name="chained_picking_type">out</field>
            <field name="chained_journal_id" ref="stock.journal_delivery"/>
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
        </record>
        <record id="stock_location_stock_HEDefault" model="stock.location">
            <field name="name">Stock</field>
            <field name="location_id" ref="stock_location_HEDefault"/>
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
        </record>

    <!--
        Resource: stock.warehouse
    -->
        <record id="warehouse0_HEDefault" model="stock.warehouse">
            <field name="name">HE Default</field>
            <field name="lot_input_id" ref="stock_location_stock_HEDefault"/>
            <field name="lot_stock_id" ref="stock_location_stock_HEDefault"/>
            <field name="lot_output_id" ref="stock_location_output_HEDefault"/>
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
        </record>

    <!--
        Resource: sale.shop
    -->
        <record id="shop_HEDefault" model="sale.shop">
            <field name="company_id" ref="company_HoldingsEuropeLtd"/>
            <field name="warehouse_id" ref="warehouse0_HEDefault"/>
            <field name="payment_default_id" ref="account.account_payment_term"/>
            <!--field name="pricelist_id" ref="product.list0"/ -->
            <field name="name">HE Default</field>
        </record>

    </data>
</openerp>
